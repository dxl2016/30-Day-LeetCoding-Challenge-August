# The rand7() API is already defined for you.
# def rand7():
# @return a random integer in the range 1 to 7

class Solution:
    def rand10(self):
        """
        :rtype: int
        """
        while True:
            random_num = ( rand7() - 1 ) * 7 + rand7()
            if random_num <= 40:
                return random_num%10 + 1

